{
  "name": "@pui/react",
  "version": "0.0.2",
  "description": "pui:react",
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "lint": "eslint --ignore-path=.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint-fix": "eslint --fix --ignore-path=.eslintignore --ext .js,.jsx,.ts,tsx .",
    "start-storybook": "start-storybook -p 6006 -s public --no-manager-cache --docs",
    "build-storybook": "build-storybook -s public --docs",
    "build-es6": "rm -rdf ./es && tsc -p tsconfig-build-es6.json && copyfiles -u 1 \"./src/**/*.scss\" \"./src/**/*.js\" \"./src/assets/**/*\" \"./src/styles/**/*\" \"./src/assets/**/*\" es && rm ./es/**/**/*.stories.scss",
    "build-es5": "babel --delete-dir-on-start src -d lib  --config-file ./babelrc-es5 --extensions .js,.ts,.tsx --copy-files --no-copy-ignored && tsc -p tsconfig-build-es5.json && rm ./lib/**/**/*.stories.scss",
    "build-all": "npm run build-es5 && npm run build-es6",
    "version-unpublish": "npm unpublish @pui/react@0.0.2 --registry http://52.83.74.69:4111 --force",
    "version-publish": "npm publish --registry http://52.83.74.69:4111",
    "test": "jest"
  },
  "changelog": {
    "repo": "https://gitlab.porsche-preview.cn/porsche-digital-china/web/pui/libs/react",
    "labels": {
      "enhancement": ":rocket Enhancement",
      "fix": ":bug: Bug Fix",
      "doc": "Refine Doc",
      "feat": "New Feature",
      "style": "Style change"
    },
    "cacheDir": ".changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run test -- -o"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "formator": {
    "name": "Prettier",
    "class": "prettier_service.js",
    "filetypes": [
      "less",
      "sass",
      "scss",
      "ts"
    ]
  },
  "keywords": [],
  "author": "Porsche Digital China Web: to be the best web frontend team all arount the world",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/cli": "^7.13.10",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/react": "^6.1.14",
    "@storybook/theming": "^6.1.14",
    "@storybook/addon-docs": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-viewport": "^6.1.14",
    "@storybook/addons": "^6.1.14",
    "@storybook/client-api": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/preset-typescript": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.31",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "husky": "^4.3.0",
    "lerna-changelog": "^1.0.1",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1",
    "typescript": "^4.1.3",
    "ts-jest": "^26.4.4",
    "sass-resources-loader": "^2.1.1",
    "@testing-library/react": "^11.2.2",
    "@lottiefiles/react-lottie-player": "3.0.4",
    "jest": "^26.6.3",
    "copyfiles": "^2.4.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "@pui/icons": "latest",
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6",
    "async-validator": "^3.5.1"
  }
}
