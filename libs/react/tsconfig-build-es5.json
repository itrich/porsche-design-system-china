{
  "compilerOptions": {
      "skipLibCheck": true,
      "jsx": "react",
      "target": "es5",
      "declaration": true,
      "emitDeclarationOnly": true,
      "isolatedModules": false,
      "moduleResolution": "node",
      "esModuleInterop":true,
      "outDir":"lib",
  },
  "include": ["src/**/*"],
  "exclude": ["src/**/*.stories.tsx","src/**/*.stories.scss"],
}